{
  "permissions": {
    "allow": [
      "WebFetch(domain:py-questdb-client.readthedocs.io)",
      "Bash(tree:*)",
      "Bash(git add:*)",
      "Bash(git commit:*)",
      "Bash(git branch:*)",
      "Bash(mkdir:*)",
      "Bash(git mv:*)",
      "Bash(rmdir:*)",
      "Bash(git rm:*)",
      "Bash(if [ -d ui ])",
      "Bash(then mkdir -p frontend)",
      "Bash(fi)",
      "Bash(if [ -f ui/FRONTEND.md ])",
      "Bash(then git mv ui/FRONTEND.md frontend/ARCHITECTURE.md)",
      "Bash(if [ -d audit ])",
      "Bash(then git mv audit/* archive/)",
      "Bash(if [ -d bugs ])",
      "Bash(then git mv bugs/* archive/)",
      "Bash(if [ -d features ])",
      "Bash(then git mv features/* archive/)",
      "Bash(if [ -d goals ])",
      "Bash(then git mv goals/* archive/)",
      "Bash(if [ -d templates ])",
      "Bash(then git rm -r templates/)",
      "Bash(pytest:*)"
    ],
    "deny": [],
    "ask": []
  }
}
