{
  "dashboard": {
    "id": null,
    "uid": "strategy-performance",
    "title": "Strategy Performance Dashboard",
    "tags": ["strategy", "performance", "signals"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "10s",
    "time": {
      "from": "now-24h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["10s", "30s", "1m", "5m", "15m"],
      "time_options": ["1h", "6h", "24h", "7d", "30d"]
    },
    "templating": {
      "list": [
        {
          "name": "strategy",
          "type": "query",
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          },
          "query": "label_values(signal_generated_total, strategy_name)",
          "multi": true,
          "includeAll": true,
          "allValue": ".*",
          "refresh": 1,
          "current": {
            "selected": true,
            "text": "All",
            "value": "$__all"
          }
        },
        {
          "name": "symbol",
          "type": "query",
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          },
          "query": "label_values(orders_submitted_total, symbol)",
          "multi": true,
          "includeAll": true,
          "allValue": ".*",
          "refresh": 1,
          "current": {
            "selected": true,
            "text": "All",
            "value": "$__all"
          }
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "type": "bargauge",
        "title": "Signals Generated per Strategy (24h)",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        },
        "targets": [
          {
            "expr": "sum by (strategy_name) (increase(signal_generated_total{strategy_name=~\"$strategy\", symbol=~\"$symbol\"}[24h]))",
            "legendFormat": "{{strategy_name}}",
            "refId": "A"
          }
        ],
        "options": {
          "orientation": "horizontal",
          "displayMode": "gradient",
          "showUnfilled": true
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "decimals": 0,
            "color": {
              "mode": "palette-classic"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": null,
                  "color": "green"
                },
                {
                  "value": 50,
                  "color": "yellow"
                },
                {
                  "value": 100,
                  "color": "red"
                }
              ]
            }
          }
        },
        "description": "Total signals generated by each strategy in the last 24 hours. Higher is not always better - quality over quantity"
      },
      {
        "id": 2,
        "type": "timeseries",
        "title": "Signal Generation Rate (Signals/hour)",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        },
        "targets": [
          {
            "expr": "sum by (strategy_name) (rate(signal_generated_total{strategy_name=~\"$strategy\", symbol=~\"$symbol\"}[5m])) * 3600",
            "legendFormat": "{{strategy_name}}",
            "refId": "A"
          }
        ],
        "options": {},
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "decimals": 1,
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "description": "Real-time signal generation rate per strategy in signals per hour"
      },
      {
        "id": 3,
        "type": "piechart",
        "title": "Signal Distribution by Strategy",
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 0,
          "y": 8
        },
        "targets": [
          {
            "expr": "sum by (strategy_name) (increase(signal_generated_total{strategy_name=~\"$strategy\", symbol=~\"$symbol\"}[24h]))",
            "legendFormat": "{{strategy_name}}",
            "refId": "A"
          }
        ],
        "options": {
          "pieType": "pie",
          "displayLabels": ["name", "percent"],
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "values": ["value", "percent"]
          }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "description": "Distribution of signals across strategies. Helps identify dominant strategies"
      },
      {
        "id": 4,
        "type": "gauge",
        "title": "Win Rate (Orders with Profit)",
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 8,
          "y": 8
        },
        "targets": [
          {
            "expr": "(sum(unrealized_pnl_usd{symbol=~\"$symbol\"} > 0) / sum(positions_open_total{symbol=~\"$symbol\"})) * 100",
            "refId": "A"
          }
        ],
        "options": {
          "showThresholdLabels": true,
          "showThresholdMarkers": true
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "decimals": 1,
            "min": 0,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": null,
                  "color": "red"
                },
                {
                  "value": 40,
                  "color": "yellow"
                },
                {
                  "value": 55,
                  "color": "green"
                }
              ]
            }
          }
        },
        "description": "Percentage of positions currently in profit. Calculated from unrealized P&L > 0. Target: > 55%"
      },
      {
        "id": 5,
        "type": "stat",
        "title": "Average P&L per Position",
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 16,
          "y": 8
        },
        "targets": [
          {
            "expr": "sum(unrealized_pnl_usd{symbol=~\"$symbol\"}) / sum(positions_open_total{symbol=~\"$symbol\"})",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "background",
          "textMode": "value_and_name"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "currencyUSD",
            "decimals": 2,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": null,
                  "color": "red"
                },
                {
                  "value": 0,
                  "color": "green"
                },
                {
                  "value": 50,
                  "color": "blue"
                }
              ]
            }
          }
        },
        "description": "Average unrealized P&L per open position. Green if positive, red if negative"
      },
      {
        "id": 6,
        "type": "table",
        "title": "Strategy Performance Summary",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        },
        "targets": [
          {
            "expr": "sum by (strategy_name) (increase(signal_generated_total{strategy_name=~\"$strategy\", symbol=~\"$symbol\"}[24h]))",
            "format": "table",
            "instant": true,
            "refId": "A"
          }
        ],
        "options": {
          "showHeader": true,
          "sortBy": [
            {
              "displayName": "Signals",
              "desc": true
            }
          ]
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "align": "auto",
              "displayMode": "auto"
            }
          },
          "overrides": [
            {
              "matcher": {
                "id": "byName",
                "options": "Value"
              },
              "properties": [
                {
                  "id": "displayName",
                  "value": "Signals (24h)"
                },
                {
                  "id": "custom.displayMode",
                  "value": "color-background"
                },
                {
                  "id": "thresholds",
                  "value": {
                    "mode": "absolute",
                    "steps": [
                      {
                        "value": null,
                        "color": "green"
                      },
                      {
                        "value": 20,
                        "color": "yellow"
                      },
                      {
                        "value": 50,
                        "color": "orange"
                      },
                      {
                        "value": 100,
                        "color": "red"
                      }
                    ]
                  }
                }
              ]
            }
          ]
        },
        "description": "Summary table of signals generated per strategy with color-coded intensity"
      },
      {
        "id": 7,
        "type": "graph",
        "title": "Signal Confidence Distribution",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(signal_confidence_bucket{strategy_name=~\"$strategy\", symbol=~\"$symbol\"}[5m])) by (le, strategy_name))",
            "legendFormat": "{{strategy_name}} - p50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(signal_confidence_bucket{strategy_name=~\"$strategy\", symbol=~\"$symbol\"}[5m])) by (le, strategy_name))",
            "legendFormat": "{{strategy_name}} - p95",
            "refId": "B"
          }
        ],
        "options": {},
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "decimals": 2,
            "color": {
              "mode": "palette-classic"
            },
            "min": 0,
            "max": 1
          }
        },
        "description": "Signal confidence distribution by strategy. Higher confidence should correlate with better performance"
      },
      {
        "id": 8,
        "type": "timeseries",
        "title": "Cumulative P&L Over Time",
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 24
        },
        "targets": [
          {
            "expr": "sum(unrealized_pnl_usd{symbol=~\"$symbol\"})",
            "legendFormat": "Total P&L",
            "refId": "A"
          }
        ],
        "options": {},
        "fieldConfig": {
          "defaults": {
            "unit": "currencyUSD",
            "decimals": 2,
            "color": {
              "mode": "thresholds"
            },
            "custom": {
              "drawStyle": "line",
              "lineWidth": 2,
              "fillOpacity": 20
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": null,
                  "color": "red"
                },
                {
                  "value": 0,
                  "color": "green"
                }
              ]
            }
          }
        },
        "description": "Cumulative unrealized P&L over time. Shows overall strategy performance trend"
      },
      {
        "id": 9,
        "type": "stat",
        "title": "Total Signals (24h)",
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 0,
          "y": 32
        },
        "targets": [
          {
            "expr": "sum(increase(signal_generated_total{strategy_name=~\"$strategy\", symbol=~\"$symbol\"}[24h]))",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "none",
          "colorMode": "value",
          "textMode": "value_and_name"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "decimals": 0,
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": null,
                  "color": "blue"
                }
              ]
            }
          }
        },
        "description": "Total signals generated across all selected strategies in the last 24 hours"
      },
      {
        "id": 10,
        "type": "stat",
        "title": "Active Strategies",
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 6,
          "y": 32
        },
        "targets": [
          {
            "expr": "count(count by (strategy_name) (signal_generated_total{strategy_name=~\"$strategy\"}))",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "none",
          "colorMode": "value",
          "textMode": "value_and_name"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "decimals": 0,
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": null,
                  "color": "blue"
                }
              ]
            }
          }
        },
        "description": "Number of unique strategies that have generated signals"
      },
      {
        "id": 11,
        "type": "stat",
        "title": "Sharpe Ratio (Estimated)",
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 12,
          "y": 32
        },
        "targets": [
          {
            "expr": "avg_over_time(unrealized_pnl_usd{symbol=~\"$symbol\"}[24h]) / stddev_over_time(unrealized_pnl_usd{symbol=~\"$symbol\"}[24h])",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "none",
          "colorMode": "background",
          "textMode": "value_and_name"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "decimals": 2,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": null,
                  "color": "red"
                },
                {
                  "value": 0.5,
                  "color": "yellow"
                },
                {
                  "value": 1,
                  "color": "green"
                },
                {
                  "value": 2,
                  "color": "blue"
                }
              ]
            }
          }
        },
        "description": "Estimated Sharpe Ratio (mean return / stddev of returns). > 1.0 is good, > 2.0 is excellent. Based on 24h data"
      },
      {
        "id": 12,
        "type": "stat",
        "title": "Profit Factor",
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 18,
          "y": 32
        },
        "targets": [
          {
            "expr": "sum(unrealized_pnl_usd{symbol=~\"$symbol\"} > 0) / abs(sum(unrealized_pnl_usd{symbol=~\"$symbol\"} < 0))",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "none",
          "colorMode": "background",
          "textMode": "value_and_name"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "decimals": 2,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": null,
                  "color": "red"
                },
                {
                  "value": 1,
                  "color": "yellow"
                },
                {
                  "value": 1.5,
                  "color": "green"
                },
                {
                  "value": 2,
                  "color": "blue"
                }
              ]
            }
          }
        },
        "description": "Profit Factor (gross profit / gross loss). > 1.5 is good, > 2.0 is excellent"
      }
    ],
    "annotations": {
      "list": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          },
          "enable": true,
          "expr": "changes(signal_generated_total[5m]) > 10",
          "iconColor": "blue",
          "name": "High Signal Activity",
          "step": "5m",
          "tagKeys": "strategy_name",
          "textFormat": "{{strategy_name}} generated >10 signals",
          "titleFormat": "Signal Burst"
        }
      ]
    }
  },
  "overwrite": true
}
